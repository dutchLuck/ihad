../ihad -S, test52.dat 
echo "+++++++++++ ihad -b 26 test52.dat              +++++++++++++++"
../ihad -b 26 test52.dat 
echo "+++++++++++ ihad -b -32 test52.dat            +++++++++++++++"
../ihad -b -32 test52.dat 
